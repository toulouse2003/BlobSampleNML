<Window x:Class="BlobSampleNML.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlobSampleNML"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="275">
    <Grid Margin="0,0,0,-0.2">
        <Label x:Name="label" Content="BlobSample" HorizontalAlignment="Left" Margin="25,75,0,0" VerticalAlignment="Top" FontSize="36" Width="205"/>
        <Button x:Name="button" Content="Indsæt" HorizontalAlignment="Left" Margin="10,180,0,0" VerticalAlignment="Top" Width="80" Click="button_Click"/>
        <Button x:Name="button1" Content="Fremvis" HorizontalAlignment="Left" Margin="170,180,0,0" VerticalAlignment="Top" Width="80" Click="button1_Click"/>
        <Button x:Name="button2" Content="Luk" HorizontalAlignment="Left" Margin="90,240,0,0" VerticalAlignment="Top" Width="80" Click="button2_Click"/>

    </Grid>
</Window>
